a = int(input())
d1 = []
i = 2
while i <= a:
    if a % i == 0:
        d1.append(i)
        a = a // i
    else:
        i += 1

d2 = list(set(d1))
d2.sort()
d3 = []
for o in range(len(d2)):
    count = d1.count(d2[o])
    if count > 1:
        d3.append(str(d2[o]) + '^' + str(count))
    else:
        d3.append(str(d2[o]))

print(' * '.join(d3))